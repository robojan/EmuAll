<?xml version="1.0" encoding="utf-8"?>
<emulator>
	<name>Gameboy Emulator V1.0</name>
	<description>Gameboy colorbox emulator.</description>
	<about>Gameboy colorbox emulator made by Robbert-Jan "robojan" de jager.</about>
	<fileFilter>*.gb;*.gbc;*.cgb</fileFilter>
	<screen id="0">
		<width>160</width>
		<height>140</height>
	</screen>
	<audio>
		<sources>4</sources>
	</audio>
	<input>
		<key key="1">
			<name>A</name>
			<defaultKey>88</defaultKey>
		</key>
		<key key="2">
			<name>B</name>
			<defaultKey>90</defaultKey>
		</key>
		<key key="3">
			<name>Up</name>
			<defaultKey>315</defaultKey>
		</key>
		<key key="4">
			<name>Down</name>
			<defaultKey>317</defaultKey>
		</key>
		<key key="5">
			<name>Left</name>
			<defaultKey>314</defaultKey>
		</key>
		<key key="6">
			<name>Right</name>
			<defaultKey>316</defaultKey>
		</key>
		<key key="7">
			<name>Start</name>
			<defaultKey>13</defaultKey>
		</key>
		<key key="8">
			<name>Select</name>
			<defaultKey>8</defaultKey>
		</key>
	</input>
	<debugging>
		<cpu>
			<disassembler>
				<enabled>true</enabled>
				<breakpoint>true</breakpoint>
				<size>0x10000</size>
				<curLineId>15</curLineId><!-- Same as Register PC -->
			</disassembler>
			<step>true</step>
			<stepOut>false</stepOut>
			<stepOver>false</stepOver>
			<registers>
				<register id="10">
					<name>AF</name>
					<size>16</size>
				</register>
				<register id="11">
					<name>BC</name>
					<size>16</size>
				</register>
				<register id="12">
					<name>DE</name>
					<size>16</size>
				</register>
				<register id="13">
					<name>HL</name>
					<size>16</size>
				</register>
				<register id="14">
					<name>SP</name>
					<size>16</size>
				</register>
				<register id="15">
					<name>PC</name>
					<size>16</size>
				</register>
				<flag id="16">
					<name>Z</name>
